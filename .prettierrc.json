{
  "printWidth": 280,
  "tabWidth": 2,
  "useTabs": false,
  "semi": false,
  "vueIndentScriptAndStyle": false,
  "singleQuote": true,
  "bracketSpacing": true,
  "trailingComma": "none",
  "jsxSingleQuote": false,
  "arrowParens": "avoid",
  "insertPragma": false,
  "requirePragma": false,
  "proseWrap": "never",
  "singleAttributePerLine": false,
  "htmlWhitespaceSensitivity": "ignore",
  "endOfLine": "lf",
  "rangeStart": 0
}
